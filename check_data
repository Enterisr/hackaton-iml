import json

file_path = "data/train_test_splits/train.jsonl"
output_path = "out.json"

output = []
with open(file_path, "r") as f:
    for i, line in enumerate(f):
        if i >= 1:
            break
        obj = json.loads(line)
        output.append(obj)

with open(output_path, "w") as out_f:
    json.dump(output, out_f, indent=2)